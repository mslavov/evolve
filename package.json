{
  "name": "evolve",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "cli": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:status": "tsx src/cli/index.ts migrate status",
    "db:validate": "tsx src/cli/index.ts migrate validate",
    "db:repair": "tsx src/cli/index.ts migrate repair",
    "db:reset": "rm -f data/scoring.db && pnpm db:migrate",
    "db:fresh": "pnpm db:reset && tsx src/db/seed-bulgarian-config.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "score": "tsx src/cli/index.ts score",
    "assess": "tsx src/cli/index.ts assess",
    "improve": "tsx src/cli/index.ts improve",
    "config": "tsx src/cli/index.ts config",
    "dataset": "tsx src/cli/index.ts dataset",
    "import:csv": "tsx src/scripts/import-csv.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "latest",
    "@ai-sdk/openai": "latest",
    "@anthropic-ai/sdk": "^0.59.0",
    "@libsql/client": "^0.15.10",
    "@paralleldrive/cuid2": "^2.2.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.4",
    "openai": "^5.12.2",
    "ora": "^8.0.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "ai",
    "agents",
    "self-improving",
    "evolution",
    "optimization",
    "mastra",
    "langchain",
    "framework-agnostic",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "description": "Evolve - Self-improving AI agents that evolve with every interaction. Framework-agnostic optimization for Mastra, LangChain, and any TypeScript agent system."
}